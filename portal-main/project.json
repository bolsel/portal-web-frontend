{
  "name": "bolsel-portal-main",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/bolsel/portal-main",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/bolsel/portal-main",
        "outputPath": "dist/apps/bolsel/portal-main"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/bolsel/portal-main"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "bolsel-portal-main:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "bolsel-portal-main:build:development",
          "dev": true,
          "port": 4301
        },
        "production": {
          "buildTarget": "bolsel-portal-main:build:production",
          "dev": false,
          "port": 4302
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "bolsel-portal-main:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/bolsel/portal-main/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/bolsel/portal-main/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
